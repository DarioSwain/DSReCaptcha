{
    "name": "dario_swain/ds-recaptcha",
    "type": "library",
    "description": "Google reCaptcha FormType and Validator for Symfony applications.",
    "keywords": ["reCaptcha", "captcha", "security", "symfony", "form", "validator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya Pokamestov",
            "homepage": "http://github.com/DarioSwain"
        }
    ],
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "google/ReCAPTCHA",
                "version":"dev-master",
                "source": {
                    "url": "https://github.com/google/ReCAPTCHA.git",
                    "type": "git",
                    "reference":"master"
                },
                "autoload": {
                    "files": ["php/recaptchalib.php"]
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "google/ReCAPTCHA": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "DS\\Component\\ReCaptcha\\Composer\\ScriptHandler::replaceViews"
        ],
        "post-update-cmd": [
            "DS\\Component\\ReCaptcha\\Composer\\ScriptHandler::replaceViews"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "DS\\Component\\ReCaptcha\\": "" }
    }
}